; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "LocalizationData",
"language": "GDScript",
"path": "res://addons/localization_editor/model/LocalizationData.gd"
}, {
"base": "Reference",
"class": "LocalizationKeys",
"language": "GDScript",
"path": "res://localization/LocalizationKeys.gd"
}, {
"base": "Reference",
"class": "LocalizationPlaceholders",
"language": "GDScript",
"path": "res://localization/LocalizationPlaceholders.gd"
} ]
_global_script_class_icons={
"LocalizationData": "",
"LocalizationKeys": "",
"LocalizationPlaceholders": ""
}

[application]

config/name="Operating System"
run/main_scene="res://Boot.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://Scripts/System/Global.gd"
Running="*res://Scripts/System/Running.gd"
UserData="*res://Scripts/System/UserData.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/localization_editor/plugin.cfg" )

[input]

MouseLeft={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[locale]

test="en"
translations=PoolStringArray( "res://localization/localizations.en.translation", "res://localization/localizations.ru.translation" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
